  <ThingDef ParentName="BuildingBase">
    <defName>EnergyCollector</defName>
    <label>energy collector</label>
    <description>A modified solar panel with added dark matter. Produces much more electricity from sunlight.</description>
    <thingClass>Building</thingClass>
    <drawerType>MapMeshAndRealTime</drawerType>
    <graphicData>
      <texPath>Things/Building/Power/EnergyCollector</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(4,4)</drawSize>
      <shadowData>
        <volume>(3.5,0.75,3.4)</volume>
        <offset>(0,0,0)</offset>
      </shadowData>
      <damageData>
        <rect>(0,0.6,4,2.8)</rect>
      </damageData>
    </graphicData>
    <castEdgeShadows>true</castEdgeShadows>
    <rotatable>false</rotatable>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <pathCost>50</pathCost>
    <fillPercent>0.5</fillPercent>
    <canOverlapZones>false</canOverlapZones>
    <statBases>
      <MaxHitPoints>2650</MaxHitPoints>
      <WorkToBuild>285</WorkToBuild>
      <Flammability>0.7</Flammability>
      <Mass>25</Mass>
    </statBases>
    <tickerType>Normal</tickerType>
    <size>(4,4)</size>
    <building>
      <destroySound>BuildingDestroyed_Metal_Big</destroySound>
    </building>
    <costList>
      <DarkMatter>100</DarkMatter>
      <Steel>100</Steel>
      <ComponentIndustrial>3</ComponentIndustrial>
    </costList>
    <thingCategories>
      <li>BuildingsPower</li>
    </thingCategories>
    <minifiedDef>MinifiedThing</minifiedDef>
    <tradeability>None</tradeability>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>NonequivalentTransmutation.CompPowerPlantCollector</compClass>
        <basePowerConsumption>-1</basePowerConsumption>
        <transmitsPower>true</transmitsPower>
      </li>
      <li Class="CompProperties_Breakdownable"/>
    </comps>
    <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
    <designationCategory>Power</designationCategory>
    <designationHotKey>Misc5</designationHotKey>
    <constructEffect>ConstructMetal</constructEffect>
    <researchPrerequisites>
      <li>EnergyCollecting</li>
    </researchPrerequisites>
    <constructionSkillPrerequisite>6</constructionSkillPrerequisite>
  </ThingDef>

  <ThingDef ParentName="BuildingBase">
    <defName>EnergyCondenser</defName>
    <label>energy condenser</label>
    <description>By combining dark matter and anti-matter inside the cells of a battery, they react with each other and produce energy. The reaction doesn't seem to slow down, which means that it will keep producing energy forever.</description>
    <graphicData>
      <texPath>Things/Building/Power/EnergyCondenser</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(2, 3)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <drawerType>MapMeshAndRealTime</drawerType>
    <pathCost>42</pathCost>
    <fillPercent>0.4</fillPercent>
    <passability>PassThroughOnly</passability>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <canOverlapZones>false</canOverlapZones>
    <castEdgeShadows>true</castEdgeShadows>
    <staticSunShadowHeight>0.5</staticSunShadowHeight>
    <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
    <statBases>
      <MarketValue>0</MarketValue>
      <MaxHitPoints>16000</MaxHitPoints>
      <Flammability>0</Flammability>
      <Mass>15</Mass>
    </statBases>
    <tickerType>Normal</tickerType>
    <size>(1, 2)</size>
    <thingCategories>
      <li>BuildingsPower</li>
    </thingCategories>
    <costList>
      <Steel>100</Steel>
      <DarkMatter>100</DarkMatter>
      <AntiMatter>100</AntiMatter>
      <ComponentSpacer>1</ComponentSpacer>
    </costList>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerPlant</compClass>
        <basePowerConsumption>-80000</basePowerConsumption>
        <transmitsPower>true</transmitsPower>
      </li>
    </comps>
    <designationCategory>Power</designationCategory>
    <minifiedDef>MinifiedThing</minifiedDef>
    <tradeability>None</tradeability>
    <researchPrerequisites>
      <li>EnergyCondensing</li>
    </researchPrerequisites>
  </ThingDef>
</Defs>