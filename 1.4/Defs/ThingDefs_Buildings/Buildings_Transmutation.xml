<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef ParentName="BenchBase">
    <defName>TableTransmutation</defName>
    <label>transmutation table</label>
    <description>A table created thanks to knowledge of Alchemy. It has the ability to transmute raw resources into different ones.</description>
    <thingClass>Building_WorkTable</thingClass>
    <graphicData>
      <texPath>Things/Building/Production/TableTransmutation</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(3,1)</drawSize>
      <shadowData>
        <volume>(2.4, 1, 0.8)</volume>
      </shadowData>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <castEdgeShadows>true</castEdgeShadows>
    <stuffCategories>
      <li>Stony</li>
    </stuffCategories>
    <defaultStuff>BlocksGranite</defaultStuff>
    <costStuffCount>45</costStuffCount>
    <costList>
      <Steel>45</Steel>
    </costList>
    <altitudeLayer>Building</altitudeLayer>
    <fillPercent>0.5</fillPercent>
    <useHitPoints>True</useHitPoints>
    <statBases>
      <WorkToBuild>1500</WorkToBuild>
      <MaxHitPoints>180</MaxHitPoints>
      <Flammability>1.0</Flammability>
    </statBases>
    <size>(3,1)</size>
    <tradeability>None</tradeability>
    <designationCategory>Production</designationCategory>
    <passability>PassThroughOnly</passability>
    <pathCost>50</pathCost>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <surfaceType>Item</surfaceType>
    <inspectorTabs>
      <li>ITab_Bills</li>
    </inspectorTabs>
    <building>
      <spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
      <isMealSource>false</isMealSource>
      <buildingTags>
        <li>Production</li>
      </buildingTags>
    </building>
    <placeWorkers>
      <li>PlaceWorker_PreventInteractionSpotOverlap</li>
    </placeWorkers>
    <researchPrerequisites>
      <li>Alchemy</li>
    </researchPrerequisites>
  </ThingDef>

